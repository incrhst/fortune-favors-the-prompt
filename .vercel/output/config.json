{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/classify/?$",
			"dest": "_render"
		},
		{
			"src": "^/debug/?$",
			"dest": "_render"
		},
		{
			"src": "^/prompt/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/submit/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}