{
	"version": 3,
	"routes": [
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/?(?:/__data.json)?$",
			"dest": "/"
		},
		{
			"src": "^/api/classify/?(?:/__data.json)?$",
			"dest": "/api/classify"
		},
		{
			"src": "^/debug/?(?:/__data.json)?$",
			"dest": "/debug"
		},
		{
			"src": "^/prompt/([^/]+?)/?(?:/__data.json)?$",
			"dest": "/prompt/[id]"
		},
		{
			"src": "^/submit/?(?:/__data.json)?$",
			"dest": "/submit"
		},
		{
			"src": "/.*",
			"dest": "/![-]/catchall"
		}
	],
	"overrides": {}
}